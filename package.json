{
  "name": "personal-block-next",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass": "^4.12.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-paginate": "^6.3.0",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "dev": "cd build && web-ext run -u https://www.google.com/search?q=test",
    "ui": "react-scripts start",
    "watch": "npm-watch",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "watch": {
    "build": [
      "package.json",
      "public/",
      "src/"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "webextensions": true,
      "browser": true,
      "node": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "npm-watch": "^0.6.0"
  }
}
